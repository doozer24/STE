{
  "name": "sevis-challenge-frontend",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0 --disable-host-check --env=${DEPLOY_ENV}",
    "build": "ng build --dev",
    "test": "ng test",
    "test:selenium": "protractor protractor.conf",
    "test:debug": "ng test --sourcemaps=false",
    "test:coverage": "ng test -cc -sr",
    "test:chrome:debug": "ng test --browser Chrome --sourcemaps=false",
    "test:headless": "ng test --browser ChromeNoSandbox --single-run",
    "test:chrome": "ng test --browser Chrome --single-run",
    "test:prod": "npm test -- --browsers ChromiumNoSandbox --single-run && npm run e2e -- --no-delete-output-path",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:prod": "ng build --prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cdk": "^5.2.5",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/flex-layout": "^5.0.0-beta.14",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "core-js": "^2.4.1",
    "lodash": "^4.17.10",
    "primeng": "^5.2.5",
    "rxjs": "^5.5.6",
    "uswds": "^1.6.2",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.1",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "axe-webdriverjs": "^0.2.0",
    "selenium-webdriver": "^2.46.1",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-junit-reporter": "~1.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "protractor-accessibility-plugin": "^0.3.0",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3",
    "jasmine-reporters": "~2.3.1"
  }
}
